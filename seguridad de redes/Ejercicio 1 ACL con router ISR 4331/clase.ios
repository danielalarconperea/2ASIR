! alpedrete
en
conf t
h alpedrete
int g0/0/0
ip address 192.168.0.1 255.255.255.0
no sh

int g0/0/1
ip address 192.168.255.5 255.255.255.252
no sh

! OSPF
router ospf 10
network 192.168.0.0 0.0.0.255 area 0
network 192.168.255.4 0.0.0.3 area 0
network 192.168.255.0 0.0.0.3 area 0

! ACL
ip access-list extended REDA
permit tcp 192.168.0.0 0.0.0.255 192.168.2.0 0.0.0.255 lt 1024
! lt: less than
permit udp 192.168.0.0 0.0.0.255 192.168.2.0 0.0.0.255 lt 1024
permit ip 192.168.0.0 0.0.0.255 192.168.1.0 0.0.0.255
deny ip any any
int g0/0/0
ip access-group REDA in


! vty
line vty 0 4
access-class 1 in 
password cisco
login
exit

access-list 1 permit 192.168.255.130 0.0.0.0
access-list 1 deny any


! CHINCHON
en
conf t
h CHINCHON
int g0/0
ip address 192.168.255.6 255.255.255.252
no sh

int g0/1
ip address 192.168.1.1 255.255.255.0
no sh

router ospf 10
network 192.168.1.0 0.0.0.255 area 0
network 192.168.255.4 0.0.0.3 area 0
network 192.168.255.128 0.0.0.127 area 0
network 192.168.255.8 0.0.0.3 area 0


! al ser 100 es estendida
access-list 100 deny ip 192.168.1.0 0.0.0.255 192.168.2.30 0.0.0.1
access-list 100 deny ip 192.168.1.0 0.0.0.255 192.168.2.32 0.0.0.31
access-list 100 permit tcp 192.168.1.0 0.0.0.255 192.168.3.0 0.0.0.255 eq 80
access-list 100 permit tcp 192.168.1.0 0.0.0.255 192.168.3.0 0.0.0.255 eq 443
access-list 100 permit tcp 192.168.1.0 0.0.0.255 192.168.3.0 0.0.0.255 eq 53
access-list 100 permit udp 192.168.1.0 0.0.0.255 192.168.3.0 0.0.0.255 eq 53
! eq: equal to
access-list 100 deny ip 192.168.1.0 0.0.0.255 192.168.3.0 0.0.0.255
access-list 100 permit ip any any
int g0/1
ip access-group 100 in


! vty
line vty 0 4
access-class 1 in 
password cisco
login
exit

access-list 1 permit 192.168.255.130 0.0.0.0
access-list 1 deny any


! R2
en
conf t
h R2

router ospf 10
network 192.168.255.0 0.0.0.3 area 0
network 192.168.255.8 0.0.0.3 area 0
network 192.168.2.0 0.0.0.255 area 0
network 192.168.3.0 0.0.0.255 area 0

! vty
line vty 0 4
access-class 1 in 
password cisco
login
exit

access-list 1 permit 192.168.255.130 0.0.0.0
access-list 1 deny any
